<?xml version="1.0" encoding="utf-8" standalone="no"?>
<config>
  <pxptab>
    <image_grid_rows>3</image_grid_rows>
    <image_grid_columns>3</image_grid_columns>
  </pxptab>
  <map_window>
    <map_chart>
      <!-- 比例 x:y -->
      <map_proportion name="x">1</map_proportion>
      <map_proportion name="y">1</map_proportion>
      <!-- 顯示格線 -->
      <grid_line_display>true</grid_line_display>
      <!-- Map底格線間隔模式 0: FixSize , 1: EachCellCount-->
      <grid_line_mode>1</grid_line_mode>
      <grid_line_set>
        <fix>
          <symbol>m</symbol>
          <cd>1</cd>
          <md>1</md>
        </fix>
        <count>
          <cd>5</cd>
          <md>5</md>
        </count>
      </grid_line_set>
      <!-- 紀錄 MD 或 CD 為 Bottom Axes  -->
      <bottom_axes>CD</bottom_axes>
      <!-- 垂直翻轉 -->
      <md_inver>false</md_inver>
      <!-- 水平翻轉 -->
      <cd_inver>false</cd_inver>
    </map_chart>
    <!-- 上一次設定的 FlawLegend-->
    <flaw_legend>
      <flaw_type>1000</flaw_type>
      <name>Not Classified</name>
      <color>#000000</color>
      <shape>Triangle</shape>
    </flaw_legend>
    <flaw_legend>
      <flaw_type>1005</flaw_type>
      <name>正面刮傷</name>
      <color>#000000</color>
      <shape>Circle</shape>
    </flaw_legend>
    <flaw_legend>
      <flaw_type>1006</flaw_type>
      <name>正面斑點雜質</name>
      <color>#FFD700</color>
      <shape>Circle</shape>
    </flaw_legend>
    <flaw_legend>
      <flaw_type>1007</flaw_type>
      <name>正面刮傷-1</name>
      <color>#98FB98</color>
      <shape>Circle</shape>
    </flaw_legend>
    <flaw_legend>
      <flaw_type>1008</flaw_type>
      <name>正面墊傷</name>
      <color>#FFFF00</color>
      <shape>Circle</shape>
    </flaw_legend>
    <flaw_legend>
      <flaw_type>1009</flaw_type>
      <name>背面刮傷</name>
      <color>#0000FF</color>
      <shape>Circle</shape>
    </flaw_legend>
    <flaw_legend>
      <flaw_type>1010</flaw_type>
      <name>背面斑點雜質</name>
      <color>#800080</color>
      <shape>Circle</shape>
    </flaw_legend>
    <flaw_legend>
      <flaw_type>1011</flaw_type>
      <name>背面刮傷-1</name>
      <color>#0000FF</color>
      <shape>Circle</shape>
    </flaw_legend>
    <flaw_legend>
      <flaw_type>1012</flaw_type>
      <name>背面墊傷</name>
      <color>#00005E</color>
      <shape>Circle</shape>
    </flaw_legend>
  </map_window>
</config>