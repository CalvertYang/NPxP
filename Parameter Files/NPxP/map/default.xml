<?xml version="1.0" encoding="utf-8" standalone="no"?>
<config>
  <pxptab>
    <image_grid_rows>3</image_grid_rows>
    <image_grid_columns>3</image_grid_columns>
  </pxptab>
  <map_window>
    <map_chart>
      <!-- 比例 x:y -->
      <map_proportion name="x">1</map_proportion>
      <map_proportion name="y">1</map_proportion>
      <!-- 顯示格線 -->
      <grid_line_display>1</grid_line_display>
      <!-- Map底格線間隔模式 0: EachCellSize , 1: EachCellCount-->
      <grid_line_mode>0</grid_line_mode>
      <!-- 紀錄 MD 或 CD 為 Bottom Axes  -->
      <bottom_axes>CD</bottom_axes>
      <!-- 垂直翻轉 -->
      <md_inver>0</md_inver>
      <!-- 水平翻轉 -->
      <cd_inver>0</cd_inver>
    </map_chart>
    <!-- 顯示項目 0:All, 1:Pass, 2:Fail -->
    <filter_display>0</filter_display>
    <!-- 上一次設定的 FlawLegend-->
    <flaw_legend>
      <flaw_type>0</flaw_type>
      <name>Not Classified</name>
      <color>#0000FF</color>
      <shape>Cross</shape>
      <checked>1</checked>
    </flaw_legend>
    <flaw_legend>
      <flaw_type>1</flaw_type>
      <name>Not Classified</name>
      <color>#00FF00</color>
      <shape>Cross</shape>
      <checked>1</checked>
    </flaw_legend>
  </map_window>
</config>
