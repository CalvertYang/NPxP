<?xml version="1.0" encoding="utf-8" standalone="no"?>
<config>
  <pxptab>
    <image_grid_rows>2</image_grid_rows>
    <image_grid_columns>1</image_grid_columns>
  </pxptab>
  <map_window>
    <map_chart>
      <!-- 比例 x:y -->
      <map_proportion name="x">3</map_proportion>
      <map_proportion name="y">4 </map_proportion>
      <!-- 顯示格線 -->
      <grid_line_display>true</grid_line_display>
      <!-- Map底格線間隔模式 0: FixSize , 1: EachCellCount-->
      <grid_line_mode>1</grid_line_mode>
      <grid_line_set>
        <fix>
          <symbol>m</symbol>
          <cd>0.2</cd>
          <md>0.2</md>
        </fix>
        <count>
          <cd>3</cd>
          <md>3</md>
        </count>
      </grid_line_set>
      <!-- 紀錄 MD 或 CD 為 Bottom Axes  -->
      <bottom_axes>CD</bottom_axes>
      <!-- 垂直翻轉 -->
      <md_inver>false</md_inver>
      <!-- 水平翻轉 -->
      <cd_inver>true</cd_inver>
    </map_chart>
    <!-- 上一次設定的 FlawLegend-->
    <flaw_legend>
      <flaw_type>0</flaw_type>
      <name>Not Classified</name>
      <color>#0000FF</color>
      <shape>Cross</shape>
      <checked>1</checked>
    </flaw_legend>
    <flaw_legend>
      <flaw_type>1</flaw_type>
      <name>Not Classified</name>
      <color>#00FF00</color>
      <shape>Cross</shape>
      <checked>1</checked>
    </flaw_legend>
  </map_window>
</config>