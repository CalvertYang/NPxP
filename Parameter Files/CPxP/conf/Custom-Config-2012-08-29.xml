<?xml version="1.0" encoding="utf-8"?>
<Config>
  <PxPVariable></PxPVariable>
  <MapVariable>
    <ImgRowsSet>1</ImgRowsSet>
    <ImgColsSet>1</ImgColsSet>
    <MapProportion>0</MapProportion>
    <!--0->1:1, 2->1:1, 2->2:1, 3->4:3, 4->3:4,  5->16:9-->
    <ShowGridSet>1</ShowGridSet>
    <!--顯示格線-->
    <MapGridSet>1</MapGridSet>
    <!--選擇使用的Grid間隔依據 0: EachCellSize , 1: EachCellCount-->
    <MapMDSet>3</MapMDSet>
    <MapCDSet>3</MapCDSet>
    <SeriesSet>0</SeriesSet>
    <!--紀錄選用的紀錄方式-->
    <BottomAxe>1</BottomAxe>
    <!--紀錄MD或CD為Bottom Axes 0:CD, 1:MD-->
    <MDInver>0</MDInver>
    <!--紀錄是否反轉座標軸-->
    <CDInver>1</CDInver>
    <ShowFlag>0</ShowFlag>
    <!--紀錄顯示項目 0:All, 1:Pass, 2:Fail-->
    <LastMapCDConvertion>1</LastMapCDConvertion>
    <LastMapMDConvertion>1</LastMapMDConvertion>
    <!--紀錄上一次單位比例-->
    <FlawTypeName>
      <FlawType>0</FlawType>
      <Name>A</Name>
      <Color>#FF0000</Color>
      <Shape>Cross</Shape>
      <Display>1</Display>
    </FlawTypeName>
    <FlawTypeName>
      <FlawType>1</FlawType>
      <Name>dark   spot</Name>
      <Color>#282828</Color>
      <Shape>Star</Shape>
      <Display>1</Display>
    </FlawTypeName>
  </MapVariable>
  <SystemVariable>
    <Language>en</Language>
    <!--en, zh, de, ko-->
  </SystemVariable>
</Config>